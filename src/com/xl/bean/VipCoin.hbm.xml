<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.xl.bean.VipCoin" table="vip_coin" schema="" catalog="xl_database">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="50"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="22"/>
        </property>
        <property name="month">
            <column name="month" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>