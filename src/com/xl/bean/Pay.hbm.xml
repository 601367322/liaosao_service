<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.xl.bean.Pay" table="pay" schema="" catalog="xl_database">
        <id name="out_trade_no">
            <column name="out_trade_no" sql-type="varchar" length="50" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" length="50"  sql-type="varchar"/>
        </property>
        <property name="body">
            <column name="body" length="50"  sql-type="varchar"/>
        </property>
        <property name="create_time">
            <column name="create_time" length="50"  sql-type="varchar"/>
        </property>
        <property name="transaction_id">
            <column name="transaction_id" length="50"  sql-type="varchar"/>
        </property>
        <property name="pay_type">
            <column name="pay_type" length="50"  sql-type="varchar"/>
        </property>
        <property name="total_fee">
            <column name="total_fee" sql-type="double" precision="22"/>
        </property>
        <property name="trade_state">
            <column name="trade_state" length="50"  sql-type="varchar"/>
        </property>
    </class>
</hibernate-mapping>